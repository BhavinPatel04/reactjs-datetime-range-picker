{
  "name": "reactjs-datetime-range-picker",
  "description": "ReactJS datetime range picker with daily, weekly, monthly, quarterly &amp; yearly levels",
  "private": false,
  "version": "0.0.2",
  "main": "./dist/reactjs-datetime-range-picker.umd.cjs",
  "module": "./dist/reactjs-datetime-range-picker.js",
  "type": "module",
  "scripts": {
    "build": "yarn clean && tsc && vite build",
    "clean": "rm -rf dist",
    "eslint": "eslint",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "generate-css": "lessc src/components/reactjs-datetime-range-picker/DateTimeRangePicker.less src/components/reactjs-datetime-range-picker/DateTimeRangePicker.css",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint --fix src/**/*.{jsx,ts,tsx}",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "storybook:start-dev": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "test",
    "tsc": "tsc",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/BhavinPatel04/reactjs-datetime-range-picker.git"
  },
  "keywords": [
    "reactjs-datetime-range-picker"
  ],
  "author": "Bhavin Patel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BhavinPatel04/reactjs-datetime-range-picker/issues"
  },
  "homepage": "https://github.com/BhavinPatel04/reactjs-datetime-range-picker#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn  eslint",
      "yarn  format"
    ]
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/reactjs-datetime-range-picker.js",
      "require": "./dist/reactjs-datetime-range-picker.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-onboarding": "1.0.11",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "moment": "^2.30.1",
    "postcss-import": "^16.0.1",
    "postcss-nesting": "^12.0.4",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "storybook": "^7.6.17",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "*",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-linter": "^2.1.1",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "@nextui-org/react": "^2.2.10",
    "framer-motion": "^11.0.8",
    "global": "^4.4.0"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.2.3",
    "jackspeak": "2.1.1"
  }
}
